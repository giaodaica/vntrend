RewriteEngine On

# Nếu yêu cầu không phải là một tệp, thư mục hoặc liên kết hợp lệ
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-l

# Kiểm tra xem có phải URL có dấu / ở cuối hay không
RewriteCond %{REQUEST_URI} ^/Virals/babythree/$
RewriteRule ^(.*)/$ /Virals/babythree [L,R=301]

# Chuyển tất cả các yêu cầu khác về index.php
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
